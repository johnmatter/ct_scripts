        },
        {
            "name" : "pScinDidCoin",
            "cut"  : "pScinShouldCoin && \"P.dc.ntrack>0\""
        },
        {
            "name" : "pScinShouldh",
            "cut"  : "pScinShould && pPIDCut && hPIDCut"
        },
        {
            "name" : "pScinDidh",
            "cut"  : "pScinDid && pPIDCut && hPIDCut"
        },
        {
            "name" : "hScinGood",
            "cut"  : "\"H.hod.goodscinhit==1\""
        },
        {
            "name" : "hGoodBeta",
            "cut"  : "\"H.hod.betanotrack > 0.5 && H.hod.betanotrack < 1.4\""
        },
        {
            "name" : "hDC1NoLarge",
            "cut"  : "\"(H.dc.1x1.nhit + H.dc.1u2.nhit + H.dc.1u1.nhit + H.dc.1v1.nhit + H.dc.1x2.nhit + H.dc.1v2.nhit) < 21\""
        },
        {
            "name" : "hDC2NoLarge",
            "cut"  : "\"(H.dc.2x1.nhit + H.dc.2u2.nhit + H.dc.2u1.nhit + H.dc.2v1.nhit + H.dc.2x2.nhit + H.dc.2v2.nhit) < 21\""
        },
        {
            "name" : "hDCNoLarge",
            "cut"  : "hDC1NoLarge && hDC2NoLarge"
        },
        {
            "name" : "hScinShould",
            "cut"  : "hScinGood && hGoodBeta && hDCNoLarge"
        },
        {
            "name" : "hScinDid",
            "cut"  : "hScinShould && \"H.dc.ntrack>0\""
        },
        {
            "name" : "hNoTrackPID",
            "cut"  : "\"abs(H.cal.etotnorm-1)<0.2\" && hCerCut"
