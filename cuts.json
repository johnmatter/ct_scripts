{
    "cuts" : [
        {
            "name" : "pHodoTimeCut",
            "cut"  : "\"P.hod.goodstarttime==1\""
        },
        {
            "name" : "pDeltaCut",
            "cut"  : "\"P.gtr.dp > -10 && P.gtr.dp < 15\""
        },
        {
            "name" : "pBetaCut",
            "cut"  : "\"P.gtr.beta > 0.6 && P.gtr.beta < 1.4\""
        },
        {
            "name" : "pCalCut",
            "cut"  : "\"P.cal.eprtracknorm < 0.3\""
        },
        {
            "name" : "pHGCerCut",
            "cut"  : "\"P.hgcer.npeSum < 0.1\""
        },
        {
            "name" : "pNGCerCut",
            "cut"  : "\"P.ngcer.npeSum < 0.1\""
        },
        {
            "name" : "pCerCut",
            "cut"  : "(pHGCerCut && pNGCerCut)"
        },
        {
            "name" : "pLH2EMissPMissCut",
            "cut"  : "\"P.kin.secondary.emiss_nuc < 0.1 && abs(P.kin.secondary.pmiss) < 0.1\""
        },
        {
            "name" : "pC12EMissPMissCut",
            "cut"  : "\"P.kin.secondary.emiss_nuc < 0.08 && abs(P.kin.secondary.pmiss) < 0.3\""
        },
        {
            "name" : "hHodoTimeCut",
            "cut"  : "\"H.hod.goodstarttime==1\""
        },
        {
            "name" : "hDeltaCut",
            "cut"  : "\"H.gtr.dp > -8 && H.gtr.dp < 8\""
        },
        {
            "name" : "hBetaCut",
            "cut"  : "\"H.gtr.beta > 0.8 &&  H.gtr.beta < 1.2\""
        },
        {
            "name" : "hCalCut",
            "cut"  : "\"H.cal.etottracknorm > 0.8 && H.cal.etottracknorm < 1.15\""
        },
        {
            "name" : "hCerCut",
            "cut"  : "\"H.cer.npeSum > 0.0\""
        },
        {
            "name" : "hWCutQ2_8",
            "cut"  : "\"H.kin.primary.W > 0.85 && H.kin.primary.W < 1.03\""
        },
        {
            "name" : "hWCutQ2_10",
            "cut"  : "\"H.kin.primary.W > 0.85 && H.kin.primary.W < 1.04\""
        },
        {
            "name" : "hWCutQ2_12",
            "cut"  : "\"H.kin.primary.W > 0.85 && H.kin.primary.W < 1.05\""
        },
        {
            "name" : "hWCutQ2_14",
            "cut"  : "\"H.kin.primary.W > 0.85 && H.kin.primary.W < 1.06\""
        },
        {
            "name" : "betaCut",
            "cut"  : "pBetaCut && hBetaCut"
        },
        {
            "name" : "deltaCut",
            "cut"  : "pDeltaCut && hDeltaCut"
        },
        {
            "name" : "pPIDCut",
            "cut"  : "pCerCut"
        },
        {
            "name" : "pReportPIDCut",
            "cut"  : "\"P.cal.etotnorm <= 0.6 && P.cal.etotnorm > 0.0\""
        },
        {
            "name" : "hPIDCut",
            "cut"  : "hCalCut && hCerCut"
        },
        {
            "name" : "PIDCut",
            "cut"  : "hPIDCut && pPIDCut"
        },
        {
            "name" : "hodoTimeCut",
            "cut"  : "pHodoTimeCut && hHodoTimeCut"
        },
        {
            "name" : "hPiPID",
            "cut"  : "\"H.cal.etottracknorm<0.8 && H.cer.npeSum<0.1\""
        },
        {
            "name" : "hPi",
            "cut"  : "hPiPID && hBetaCut && hDeltaCut"
        },
        {
            "name" : "hElecPID",
            "cut"  : "hCerCut && hCalCut"
        },
        {
            "name" : "hElec",
            "cut"  : "hElecPID && hBetaCut && hDeltaCut"
        },
        {
            "name" : "coinCutsC12",
            "cut"  : "betaCut && deltaCut && PIDCut && hodoTimeCut"
        },
        {
            "name" : "coinCutsLH2_Q2_8",
            "cut"  : "betaCut && deltaCut && PIDCut && hodoTimeCut && hWCutQ2_8 && pLH2EMissPMissCut"
        },
        {
            "name" : "coinCutsLH2_Q2_10",
            "cut"  : "betaCut && deltaCut && PIDCut && hodoTimeCut && hWCutQ2_10 && pLH2EMissPMissCut"
        },
        {
            "name" : "coinCutsLH2_Q2_12",
            "cut"  : "betaCut && deltaCut && PIDCut && hodoTimeCut && hWCutQ2_12 && pLH2EMissPMissCut"
        },
        {
            "name" : "coinCutsLH2_Q2_14",
            "cut"  : "betaCut && deltaCut && PIDCut && hodoTimeCut && hWCutQ2_14 && pLH2EMissPMissCut"
        },
        {
            "name" : "pScinGood",
            "cut"  : "\"P.hod.goodscinhit==1\""
        },
        {
            "name" : "pGoodBeta",
            "cut"  : "\"P.hod.betanotrack > 0.5 && P.hod.betanotrack < 1.4\""
        },
        {
            "name" : "pDC1NoLarge",
            "cut"  : "\"(P.dc.1x1.nhit + P.dc.1u2.nhit + P.dc.1u1.nhit + P.dc.1v1.nhit + P.dc.1x2.nhit + P.dc.1v2.nhit) < 21\""
        },
        {
            "name" : "pDC2NoLarge",
            "cut"  : "\"(P.dc.2x1.nhit + P.dc.2u2.nhit + P.dc.2u1.nhit + P.dc.2v1.nhit + P.dc.2x2.nhit + P.dc.2v2.nhit) < 21\""
        },
        {
            "name" : "pDCNoLarge",
            "cut"  : "pDC1NoLarge && pDC2NoLarge"
        },
        {
            "name" : "pScinShould",
            "cut"  : "pScinGood && pGoodBeta && pDCNoLarge"
        },
        {
            "name" : "pScinDid",
            "cut"  : "\"P.dc.ntrack>0\""
        },
        {
            "name" : "pScinShouldh",
            "cut"  : "pScinShould && pPIDCut"
        },
        {
            "name" : "pScinDidh",
            "cut"  : "pScinDid && pPIDCut"
        },
        {
            "name" : "hScinGood",
            "cut"  : "\"H.hod.goodscinhit==1\""
        },
        {
            "name" : "hGoodBeta",
            "cut"  : "\"H.hod.betanotrack > 0.5 && H.hod.betanotrack < 1.4\""
        },
        {
            "name" : "hDC1NoLarge",
            "cut"  : "\"(H.dc.1x1.nhit + H.dc.1u2.nhit + H.dc.1u1.nhit + H.dc.1v1.nhit + H.dc.1x2.nhit + H.dc.1v2.nhit) < 21\""
        },
        {
            "name" : "hDC2NoLarge",
            "cut"  : "\"(H.dc.2x1.nhit + H.dc.2u2.nhit + H.dc.2u1.nhit + H.dc.2v1.nhit + H.dc.2x2.nhit + H.dc.2v2.nhit) < 21\""
        },
        {
            "name" : "hDCNoLarge",
            "cut"  : "hDC1NoLarge && hDC2NoLarge"
        },
        {
            "name" : "hScinShould",
            "cut"  : "hScinGood && hGoodBeta && hDCNoLarge"
        },
        {
            "name" : "hScinDid",
            "cut"  : "\"H.dc.ntrack>0\""
        },
        {
            "name" : "hScinShoulde",
            "cut"  : "hScinShould && hPIDCut"
        },
        {
            "name" : "hScinDide",
            "cut"  : "hScinDid && hPIDCut"
        },
        {
            "name" : "pNGCerShould",
            "cut"  : "pBetaCut && hBetaCut && hCalCut && hCerCut && pHGCerCut"
        },
        {
            "name" : "pNGCerDid",
            "cut"  : "pNGCerCut"
        },
        {
            "name" : "pHGCerShould",
            "cut"  : "pBetaCut && hBetaCut && hCalCut && hCerCut && pNGCerCut"
        },
        {
            "name" : "pHGCerDid",
            "cut"  : "pHGCerCut"
        },
        {
            "name" : "pCerShould",
            "cut"  : "pBetaCut && hBetaCut && hCalCut && hCerCut"
        },
        {
            "name" : "pCerDid",
            "cut"  : "pCerCut"
        },
        {
            "name" : "hCerShould",
            "cut"  : "pBetaCut && hBetaCut && hCalCut && pCerCut"
        },
        {
            "name" : "hCerDid",
            "cut"  : "hCerCut"
        },
        {
            "name" : "pCalShould",
            "cut"  : "pBetaCut && hbetaCut && hCalCut && hCerCut && pCerCut"
        },
        {
            "name" : "pCalDid",
            "cut"  : "pCalCut"
        },
        {
            "name" : "hCalShould",
            "cut"  : "pBetaCut && hBetaCut && hCerCut && pCerCut"
        },
        {
            "name" : "hCalDid",
            "cut"  : "hCalCut"
        }
    ]
}
