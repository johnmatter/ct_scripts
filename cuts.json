        },
        {
            "name" : "pDC1NoLarge",
            "cut"  : "\"(P.dc.1x1.nhit + P.dc.1u2.nhit + P.dc.1u1.nhit + P.dc.1v1.nhit + P.dc.1x2.nhit + P.dc.1v2.nhit) < 21\""
        },
        {
            "name" : "pDC2NoLarge",
