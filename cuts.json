        },
        {
            "name" : "pDCNoLarge",
            "cut"  : "pDC1NoLarge && pDC2NoLarge"
        },
        {
            "name" : "pScinShould",
            "cut"  : "pScinGood && pGoodBeta && pDCNoLarge"
        },
        {
            "name" : "pScinDid",
            "cut"  : "pScinShould && \"P.dc.ntrack>0\""
        },
        {
            "name" : "pScinShouldCoin",
