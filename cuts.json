{
    "cuts" : [
        {
            "name" : "pHodoTimeCut",
            "cut"  : "\"P.hod.goodstarttime ==1\""
        },
        {
            "name" : "pDeltaCut",
            "cut"  : "\"P.gtr.dp > -12 && P.gtr.dp < 12\""
        },
        {
            "name" : "pBetaCut",
            "cut"  : "\"P.gtr.beta > 0.8 && P.gtr.beta < 1.3\""
        },
        {
            "name" : "pCalCut",
            "cut"  : "\"P.cal.eprtracknorm < 0.2\""
        },
        {
            "name" : "pCerCut",
            "cut"  : "\"P.hgcer.npeSum < 0.1\""
        },
        {
            "name" : "pPMissCut",
            "cut"  : "\"abs(P.kin.secondary.pmiss) < 0.4\""
        },
        {
            "name" : "hHodoTimeCut",
            "cut"  : "\"H.hod.goodstarttime ==1\""
        },
        {
            "name" : "hDeltaCut",
            "cut"  : "\"H.gtr.dp > -10 && H.gtr.dp < 10\""
        },
        {
            "name" : "hBetaCut",
            "cut"  : "\"H.gtr.beta > 0.8 &&  H.gtr.beta < 1.3\""
        },
        {
            "name" : "hCalCut",
            "cut"  : "\"H.cal.etottracknorm > 0.8 && H.cal.etottracknorm < 1.5\""
        },
        {
            "name" : "hCerCut",
            "cut"  : "\"H.cer.npeSum > 0.5\""
        },
        {
            "name" : "hWCut",
            "cut"  : "\"H.kin.primary.W > 0.75 && H.kin.primary.W < 1.15\""
        },
        {
            "name" : "betaCut",
            "cut"  : "pBetaCut && hBetaCut"
        },
        {
            "name" : "deltaCut",
            "cut"  : "pDeltaCut && hDeltaCut"
        },
        {
            "name" : "PIDCut",
            "cut"  : "pCerCut && hCalCut && hCerCut"
        },
        {
            "name" : "hodoTimeCut",
            "cut"  : "pHodoTimeCut && hHodoTimeCut"
        },
        {
            "name" : "coinCutsC12",
            "cut"  : "betaCut && deltaCut && PIDCut && hodoTimeCut && pPMissCut"
        },
        {
            "name" : "coinCutsLH2",
            "cut"  : "betaCut && deltaCut && PIDCut && hodoTimeCut && hWCut"
        },
        {
            "name" : "coinCutsAl",
            "cut"  : "betaCut && deltaCut && PIDCut && hodoTimeCut && hWCut"
        },
        {
            "name" : "pScinGood",
            "cut"  : "\"P.hod.goodscinhit==1\""
        },
        {
            "name" : "pGoodBeta",
            "cut"  : "\"P.hod.betanotrack > 0.5 && P.hod.betanotrack < 1.4\""
        },
        {
            "name" : "pDC1NoLarge",
            "cut"  : "\"(P.dc.1x1.nhit + P.dc.1u2.nhit + P.dc.1u1.nhit + P.dc.1v1.nhit + P.dc.1x2.nhit + P.dc.1v2.nhit) < 21\""
        },
        {
            "name" : "pDC2NoLarge",
            "cut"  : "\"(P.dc.2x1.nhit + P.dc.2u2.nhit + P.dc.2u1.nhit + P.dc.2v1.nhit + P.dc.2x2.nhit + P.dc.2v2.nhit) < 21\""
        },
        {
            "name" : "pDCNoLarge",
            "cut"  : "pDC1NoLarge && pDC1NoLarge"
        },
        {
            "name" : "pScinShould",
            "cut"  : "pScinGood && pGoodBeta && pDCNoLarge"
        },
        {
            "name" : "pScinDid",
            "cut"  : "pScinShould && \"P.dc.ntrack>0\""
        },
        {
            "name" : "pSinglesPID",
            "cut"  : "pCerCut"
        },
        {
            "name" : "pScinShouldh",
            "cut"  : "pScinShould && pSinglesPID"
        },
        {
            "name" : "pScinDidh",
            "cut"  : "pScinDid && pSinglesPID"
        },
        {
            "name" : "pTrackShould",
            "cut"  : "pScinShouldh"
        },
        {
            "name" : "pTrackDid",
            "cut"  : "pScinDidh"
        },
        {
            "name" : "hScinGood",
            "cut"  : "\"H.hod.goodscinhit==1\""
        },
        {
            "name" : "hGoodBeta",
            "cut"  : "\"H.hod.betanotrack > 0.5 && H.hod.betanotrack < 1.4\""
        },
        {
            "name" : "hDC1NoLarge",
            "cut"  : "\"(H.dc.1x1.nhit + H.dc.1u2.nhit + H.dc.1u1.nhit + H.dc.1v1.nhit + H.dc.1x2.nhit + H.dc.1v2.nhit) < 21\""
        },
        {
            "name" : "hDC2NoLarge",
            "cut"  : "\"(H.dc.2x1.nhit + H.dc.2u2.nhit + H.dc.2u1.nhit + H.dc.2v1.nhit + H.dc.2x2.nhit + H.dc.2v2.nhit) < 21\""
        },
        {
            "name" : "hDCNoLarge",
            "cut"  : "hDC1NoLarge && hDC1NoLarge"
        },
        {
            "name" : "hScinShould",
            "cut"  : "hScinGood && hGoodBeta && hDCNoLarge"
        },
        {
            "name" : "hScinDid",
            "cut"  : "hScinShould && \"H.dc.ntrack>0\""
        },
        {
            "name" : "hSinglesPID",
            "cut"  : "hCalCut && hCerCut"
        },
        {
            "name" : "hScinShoulde",
            "cut"  : "hScinShould && hSinglesPID"
        },
        {
            "name" : "hScinDide",
            "cut"  : "hScinDid && hSinglesPID"
        },
        {
            "name" : "hTrackShould",
            "cut"  : "hScinShoulde"
        },
        {
            "name" : "hTrackDid",
            "cut"  : "hScinDide"
        },
        {
            "name" : "pCerShould",
            "cut"  : "pBetaCut && pCalCut && \"P.dc.ntrack>0\""
        },
        {
            "name" : "pCerDid",
            "cut"  : "pCerShould && pCerCut"
        },
        {
            "name" : "hCerShould",
            "cut"  : "hBetaCut && hCalCut && \"H.dc.ntrack>0\""
        },
        {
            "name" : "hCerDid",
            "cut"  : "hCerShould && hCerCut"
        },
        {
            "name" : "pCalShould",
            "cut"  : "pBetaCut && pCerCut && \"P.dc.ntrack>0\""
        },
        {
            "name" : "pCalDid",
            "cut"  : "pCalShould && pCalCut"
        },
        {
            "name" : "hCalShould",
            "cut"  : "hBetaCut && hCerCut && \"H.dc.ntrack>0\""
        },
        {
            "name" : "hCalDid",
            "cut"  : "hCalShould && hCalCut"
        }
    ]
}
